{
    "manifest_version": 3,
    "name": "Only Read Aloud Content",
    "version": "1.0",
    "description": "Adds aria-hidden attribute to non-content text. Current supported websites: Reddit, Old Reddit.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.reddit.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  